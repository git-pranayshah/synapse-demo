{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "functions": [],
    "variables": {},
    "resources": [
       {
            "name":"[concat(concat(parameters('workspaces_working_software_name'), uniqueString(resourceGroup().id)), '/TemplateDeployment')]", 
         
            "type": "Microsoft.Synapse/workspaces/linkedservices",
            "apiVersion": "2020-12-01",
            "properties": {
                "parameters": {
                    "workspaces_working_software_name": {
                        "type": "string",
                        "defaultValue": "swsynpse"
                      },
                    "server_name": {
                        "type": "string",
                        "defaultValue": "sampledb42nwwkcdqytqi"
                    },
                    "servers_sample_db_server_mas_name": {
                        "type": "string",
                        "defaultValue": "master"
                    },
                    "servers_sql_db_awd_login_user": {
                        "type": "string",
                        "defaultValue": "sql-admin"
                    },
                    "servers_sql_db_awd_login_password": {
                        "type": "securestring",
                        "defaultValue": "Passw@rd2424"
                    }
                },
                "annotations": [],
                "type": "AzureSqlDatabase",
                "typeProperties": {
                    "connectionString": "[concat(concat(concat(concat(concat('integrated security=False;encrypt=True;connection timeout=30;data source=', parameters('server_name')), '.database.windows.net;initial catalog='),  parameters('servers_sample_db_server_mas_name')), ';user id='), parameters('servers_sql_db_awd_login_user'))]",
                    "password": "parameters('servers_sql_db_awd_login_password')"
                },
                "connectVia": {
                    "referenceName": "AutoResolveIntegrationRuntime",
                    "type": "IntegrationRuntimeReference"
                }
            }
        }
    ],
    "outputs": {
    }
  }