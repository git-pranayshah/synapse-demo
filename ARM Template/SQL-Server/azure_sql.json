
{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {},
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2021-04-01",
            "name": "linkedTemplate",
            "properties": {
              "mode": "Incremental",
              "parametersLink": {
                "uri": "https%3A%2F%2Fraw.githubusercontent.com%2Fgit-pranayshah%2Fsynapse-demo%2Fdev%2FARM%2520Template%2FParameters%2Fparameters.json"
            },
              "parameters": {
                "Subscription_Id": {
                  "value": "[parameters('Subscription_Id')]"
                }
              },
              "parameters": {
                "servers_sample_db_server_mas_name": {
                  "value": "[parameters('servers_sample_db_server_mas_name')]"
                }
              },
              "parameters": {
                "servers_sql_db_awd_login_user": {
                  "value": "[parameters('servers_sql_db_awd_login_user')]"
                }
              },
              "parameters": {
                "servers_sql_db_awd_login_password": {
                  "value": "[parameters('servers_sql_db_awd_login_password')]"
                }
              }
            }
        },        
        {
            "type": "Microsoft.Sql/servers",
            "apiVersion": "2021-08-01-preview",
            "name": "[parameters('servers_sample_db_server_mas_name')]",
            "location": "[resourceGroup().location]",
            "kind": "v12.0",
            "parametersLink": {
                "uri": "https%3A%2F%2Fraw.githubusercontent.com%2Fgit-pranayshah%2Fsynapse-demo%2Fdev%2FARM%2520Template%2FParameters%2Fparameters.json"
            },
            "properties": {
                "administratorLogin": "[parameters('servers_sql_db_awd_login_user')]",
                "administratorLoginPassword": "[parameters('servers_sql_db_awd_login_password')]",
                "version": "12.0",
                "minimalTlsVersion": "1.2",
                "publicNetworkAccess": "Enabled",
                "restrictOutboundNetworkAccess": "Disabled"
            }       
        }
    ]
}